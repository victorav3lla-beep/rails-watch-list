<div class="container m-4">
  <h1>New List</h1>
  <div class="container my-4">
    <%= simple_form_for [@list] do |f| %>
      <%= f.input :name %>
      <%= f.submit "Add", class: "btn btn-primary" %>
    <% end %>
    <div class="my-4">
      <%= link_to "Back", root_path, class: "btn btn-success" %>
    </div>
  </div>
</div>
